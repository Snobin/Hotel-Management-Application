#Field validation properties


CustomSize.users.userId=User ID length must be between {users.userId.min} and {users.userId.max}
users.userId.min=3
users.userId.max=50

CustomSize.users.userName=User Name length must be between {users.userName.min} and {users.userName.max}
users.userName.min=3
users.userName.max=50


CustomSize.users.password=Password length must be between {2} and {1}
users.password.min=3
users.password.max=36

CustomSize.users.emailId=Email ID length must be between {users.emailId.min} and {users.emailId.max}
users.emailId.min=5
users.emailId.max=45

CustomSize.users.cellNo=Mobile Number length must be between {users.cellNo.min} and {users.cellNo.max}
users.cellNo.min=9
users.cellNo.max=12

NotEmpty.users.userId=User ID cannot be empty.
NotBlank.users.userId=User ID cannot be empty.
NotBlank.users.userName=User Name cannot be empty.
Pattern.users.userID=Invalid User ID. Only Alphanumerics With Space and the following special characters (. - _ @) are Allowed
Pattern.users.userName=Invalid User Name. Only Alphanumerics With Space and the following special characters (. - _ @) are Allowed
Pattern.users.emailId=Invalid Email ID.
Pattern.users.cellNo=Invalid Mobile Number. Only Numerics Allowed
Pattern.users.bankId=Invalid Bank ID
NotEmpty.users.bankId=Bank ID cannot be empty
NotEmpty.users.orgDept=Department cannot be Empty






CustomSize.users.password=Password length must be between {2} and {1}
users.password.min=3
users.password.max=36





